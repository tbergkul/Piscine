NAME = ft_display_file.c
FILE = ft_display_file

all:
	gcc -Wall -Wextra -Werror -o $(FILE) $(NAME)
	
clean:
	/bin/rm -f a.out

fclean: 
	/bin/rm -f $(FILE)
